<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobileCongreso.RegistroConferencia">
    <ContentPage.Content>
        <StackLayout x:Name="Registro">
            <Label>Nombre:</Label>
            <Entry IsEnabled="False" x:Name="NombreEvento"></Entry>
            <Label>Hora Inicio:</Label>
            <Entry  IsEnabled="False" x:Name="HoraInicioEvento"></Entry>
            <Label>Hora Termino:</Label>
            <Entry  IsEnabled="False"  x:Name="HoraTerminoEvento"></Entry>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>